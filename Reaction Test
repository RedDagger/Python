import time
import random
Name = input("Welcome! Before we begin, what is your name? ")
print("Will begin in... 3")
time.sleep(random.randint(1,10) / 10)
print("Will begin in... 2")
time.sleep(random.randint(1,10) / 10)
print("Will begin in... 1")
time.sleep(random.randint(1,10) / 10)
Start = time.time()
input("Quick!!!")
End = time.time()
TimeT = End - Start
print("You took ", TimeT, " Seconds.")
if TimeT <= 0.338:
    print("Your reactions are above average! Well done!")
else:
    print("Your reactions are slower then average. Keep practacing.")
